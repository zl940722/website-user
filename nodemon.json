{
    "restartable": "rs",
    "ignore": [
        "\\.git",
        "node_modules/*",
        "build/*",
        "npm-debug.log",
        "node_modules/.*.*/node_modules"
    ],
    "ext": "ts,tsx,yaml,js,json,pcss",
    "execMap": {
        "ts": "ts-node",
        "tsx": "ts-node"
    },
    "env": {
        "NODE_ENV": "development"
    },
    "watch": [
        "src/server/"
    ]
}
